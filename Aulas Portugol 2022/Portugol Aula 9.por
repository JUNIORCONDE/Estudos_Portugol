programa {
  funcao inicio() {
    inteiro dataNascimento
    inteiro anoAtual
    inteiro idadeAtual
    
    escreva ("Digite sua Data de Nascimento.: ")
    leia (dataNascimento)
    escreva ("Digite Ano Atual.: ")
    leia (anoAtual)
    
    idadeAtual = anoAtual - dataNascimento

    escreva("Sua Idade Atual é.: ", idadeAtual)
   
  }
}
